<div class="page-top">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12">
                <h2 class="page-heading">
                    <span class="page-heading-title"><img alt="fashion" src="images/icons8-three.png"/> HOT</span>
                </h2>

                <div class="latest-deals-product">
                    <ul class="product-list owl-carousel" data-dots="false" data-loop="true" data-nav="true"
                        data-margin="10" data-autoplayTimeout="1000" data-autoplayHoverPause="true"
                        data-responsive='{"0":{"items":1},"600":{"items":3},"1000":{"items":5}}'>
                        <%for (var i = 0; i < hot.length; i++) {%>
                            <%var value = hot[i].price.toLocaleString(undefined,{ minimumFractionDigits: 0 });%>
                            <li>
                                <div class="left-block">
                                    <a>
                                        <img class="img-responsive" alt="product" src="<%-hot[i].image%>"/>
                                    </a>
                                    <div class="quick-view">
                                        <a title="Add to my wishlist" class="heart"></a>
                                        <a title="Add to compare" class="compare"></a>
                                        <a title="Quick view" href="<%-hot[i].image%>" class="search fancybox-button"></a>
                                    </div>
                                    <div class="add-to-cart">
                                        <a class="add-item-to-cart" title="Add to Carr" data-name="<%-hot[i].pro_name%>"
                                           data-price="<%-hot[i].price%>" data-image="<%-hot[i].image%>" data-product-code="<%-hot[i]._id%>">Add to Cart</a>
                                    </div>
                                </div>
                                <div class="right-block">
                                    <h5 class="product-name"><a href="/detail/<%-hot[i]._id%>"><%-hot[i].pro_name%></a></h5>
                                    <div class="content_price">
                                        <span class="price product-price">
                                            <span style="color: orangered">Best Price:</span> <%-value%> VNĐ</span>
                                    </div>
                                </div>
                            </li>
                        <%}%>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

